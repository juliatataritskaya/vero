{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "vero"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "icon.png"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/animate.css/animate.css",
        "../node_modules/datatables.net-bs/css/dataTables.bootstrap.css",
        "../node_modules/datatables.net-select-bs/css/select.bootstrap.css",
        "../node_modules/datatables.net-responsive-bs/css/responsive.bootstrap.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/pace-js/pace.min.js",
        "../node_modules/datatables.net/js/jquery.dataTables.js",
        "../node_modules/datatables.net-bs/js/dataTables.bootstrap.js",
        "../node_modules/datatables.net-select/js/dataTables.select.js",
        "../node_modules/datatables.net-responsive/js/dataTables.responsive.js",
        "../src/assets/js/core/plugins.js",
        "../src/assets/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js",
        "../src/assets/js/plugins/jvectormap/jquery-jvectormap-europe-mill-en.js",
        "../src/assets/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
        "../src/assets/js/plugins/jvectormap/jquery-jvectormap-de-mill.js",
        "../src/assets/js/plugins/jvectormap/jquery-jvectormap-nl-mill.js",
        "../src/assets/js/plugins/morris/raphael-min.js",
        "../src/assets/js/plugins/morris/morris.min.js",
        "../src/assets/js/plugins/owl/owl.carousel.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
